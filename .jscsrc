{
  "preset": "google",
  "validateQuoteMarks": { "mark": "'", "escape": true, "ignoreJSX": true },
  "maximumLineLength": 120,
  "requireTrailingComma": false,
  "esnext": true,
  "disallowSpacesInsideObjectBrackets": false,
  "requireSpacesInsideObjectBrackets" : "all",
  "requireSpacesInGenerator": {
    "beforeStar": true
  },
  "excludeFiles": [
    "node_modules/**",
    "coverage/**"
  ]
}
