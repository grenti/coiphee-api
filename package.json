{
  "name": "coiphee-api",
  "private": true,
  "version": "0.0.1",
  "repository": "grenti/coiphee_api",
  "description": "Coiphee API application",
  "keywords": [
    "coiphee",
    "api",
    "koa"
  ],
  "engines": {
    "node": ">=7.4.0"
  },
  "scripts": {
    "dev": "nodemon './src/server.js'",
    "test": "node --harmony-async-await src/tests/testRunner.js",
