    "joi": "^10.0.6",
    "koa": "2.0.0",
    "koa-body": "^1.4.0",
    "koa-bodyparser": "^3.2.0",
    "koa-compress": "1.0.7",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-favicon": "^1.2.0",
    "koa-jwt": "^1.2.0",
    "koa-logger": "~1.2.1",
    "koa-morgan": "^1.0.1",
    "koa-ratelimit": "~1.0.3",
